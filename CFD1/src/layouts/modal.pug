//modal login
.md-modal.md-login#modal-login
    .md-content
        h2.md-title.title.d-flex.justify-content-between Returning Customer
        a.btn-close.close
            i.fas.fa-window-close
        form.form-horizontal.login-form(action='' method='post')
            .form-group.d-flex.justify-content-between.align-items-center
                label.control-label(for='input-email') E-Mail Address
                input.form-control(type='text' name='email' value='' placeholder='E-Mail Address')
            .form-group.d-flex.justify-content-between.align-items-center
                label.control-label(for='input-password') Password
                input.form-control(type='password' name='password' value='' placeholder='Password')
            div.forget-password
                a(href='' target='_top') Forgotten Password
            .buttons
                button.btn.btn-primary(type='submit' data-loading-text='<span>Login</span>')
                    span Login
//modal register
.md-modal.md-login#modal-register
    .md-content
        h2.md-title.title.d-flex.justify-content-between Your Personal Details
        a.btn-close.close
                i.fas.fa-window-close
        form.form-horizontal.login-form(action='' method='post')
            .form-radio
                label.form-radio-name(for="") Customer Group
                .radio
                    label
                        input(type='radio' name='customer_group_id' value='2')
                        | Business
                .radio
                    label
                        input(type='radio' name='customer_group_id' value='2')
                        | Default
            .form-group.d-flex.justify-content-between.align-items-center
                label.control-label(for='input-email') First Name
                    span *
                input#input-email.form-control(type='text' name='email' value='' placeholder='First Name')
            .form-group.d-flex.justify-content-between.align-items-center
                label.control-label(for='input-password') Last Name
                    span *
                input.form-control(type='text' name='password' value='' placeholder='Last Name')
            .form-group.d-flex.justify-content-between.align-items-center
                label.control-label(for='input-password') E-mail
                    span *
                input.form-control(type='email' name='password' value='' placeholder='E-mail')
            .form-group.d-flex.justify-content-between.align-items-center
                label.control-label(for='input-password') Telephone
                    span *
                input#input-password.form-control(type='tel' name='password' value='' placeholder='Telephone')
            h2.md-title.title Your Password
            .form-group.d-flex.justify-content-between.align-items-center
                label.control-label(for='input-password') Password
                    span *
                input.form-control(type='tel' name='password' value='' placeholder='Password')
            .form-group.d-flex.justify-content-between.align-items-center
                label.control-label(for='input-password') Password Confirm
                    span *
                input.form-control(type='tel' name='password' value='' placeholder='Password Confirm')
            h2.md-title.title Your Password
            .form-radio.d-flex
                label.form-radio-name(for="") Customer Group
                .radio
                    label
                        input(type='radio' name='customer_group_id' value='2')
                        | Yes
                .radio
                    label
                        input(type='radio' name='customer_group_id' value='2')
                        | no
            .buttons.text-center
                button.btn.btn-primary(type='submit' data-loading-text='<span>Login</span>')
                    span Login 
//modal-cart
.md-modal.madal-mobile#mobile-cart
    .madal-mobile__header.d-flex-align-items-center.justify-content-center
        h3.text-uppercase your cart
        a.btn-close.close
                i.fas.fa-window-close
    .madal-mobile__content
        ul.madal-mobile__list
            li.madal-mobile__item.cart-setting__item.cart-setting__item--one
                table.table-product
                    tbody
                        tr  
                            td.td-img
                                img(src="/images/home/cartmibile-60x60.jpg", alt="")
                            td.td-text.td-link
                                a(href="") Mascara Curved Brush
                                p
                                    span color
                                    small gold
                            td.td-text x1 
                            td.td-text $72.80 
                            td.td-text
                                i.fas.fa-times-circle
            li.madal-mobile__item.cart-setting__item.cart-setting__item--two
                table
                    tbody
                        tr
                            td.td-text--bold Sub-Total 
                            td.td-text--bold $1,808.00
                        tr
                            td.td-text--bold Eco Tax (-2.00) 
                            td.td-text--bold $10.00
                        tr
                            td.td-text--bold VAT (20%) 
                            td.td-text--bold $361.60
                        tr
                            td.td-text--bold Total
                            td.td-text--bold $2,179.60
            li.madal-mobile__item3.text-center.cart-setting__item.cart-setting__item--three
                +btn('blue','View Cart')
                +btn('green','Checkout')
